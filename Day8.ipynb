   "source": [
    "## Print the following pattern:\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "7 6 5 4 3 2 1 \n",
      "$ $ $ $ $ $ \n",
      "* * * * * \n",
      "7 6 5 4 \n",
      "$ $ $ \n",
      "* * \n",
      "7 \n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter Value: \"))\n",
    "for i in range(1, n+1):\n",
    "    for j in range(n, i-1,-1):\n",
    "        if(i%3==1):\n",
    "            print(j,end=\" \")\n",
    "        elif(i%3==0):\n",
    "            print(\"*\",end=\" \")\n",
    "        else:\n",
    "            print(\"$\",end=\" \")\n",
    "    print(\"\")"
   ]
  }
